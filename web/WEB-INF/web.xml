<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>AddAccount</servlet-name>
        <servlet-class>controller.AddAccount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getVehicle</servlet-name>
        <servlet-class>controller.getVehicle</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>checkAvailableCar</servlet-name>
        <servlet-class>controller.checkAvailableCar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderTmp</servlet-name>
        <servlet-class>controller.OrderTmp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>controller.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>controller.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addOrder</servlet-name>
        <servlet-class>controller.addOrder</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddAccount</servlet-name>
        <url-pattern>/addAccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getVehicle</servlet-name>
        <url-pattern>/getVehicle</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>checkAvailableCar</servlet-name>
        <url-pattern>/checkAvailableCar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderTmp</servlet-name>
        <url-pattern>/orderTmp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addOrder</servlet-name>
        <url-pattern>/addOrder</url-pattern>
    </servlet-mapping>
</web-app>
